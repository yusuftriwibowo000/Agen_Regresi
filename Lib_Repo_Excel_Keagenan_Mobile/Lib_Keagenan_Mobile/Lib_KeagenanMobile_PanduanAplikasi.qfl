Sub BukaMenu_Akun()
	Do
	Loop until Device("Device").App("BNI Agen46").MobileObject("Akun").Exist(1)
	If Device("Device").App("BNI Agen46").MobileObject("Akun").Exist(5) Then
		Device("Device").App("BNI Agen46").MobileObject("Akun").Tap
		wait 4
		Call CaptureImageUFTV2(Device("Device").App("BNI Agen46") , "User Buka Menu Akun", " ", compatibilityMode.Mobile, ReportStatus.Done)
		wait 4
	Else
		Reporter.ReportEvent micFail, "Gagal Buka Menu AKun","Gagal"
	End If
End Sub

Sub Buka_BantuanApk()
