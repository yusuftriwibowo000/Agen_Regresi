Function Choice_Maskapai(dt_TCID)
	Do
	Loop until Device("Device").App("BNI Agen46").MobileObject("Maskapai").Exist(1)
	Device("Device").App("BNI Agen46").MobileObject("Maskapai").Tap
	If dt_TCID = "MAG0026-001" Then
		Do
		Loop until Device("Device").App("BNI Agen46").MobileObject("Citilink").Exist(1)
		If Device("Device").App("BNI Agen46").MobileObject("Citilink").Exist(5) Then
			Device("Device").App("BNI Agen46").MobileObject("Citilink").Tap
			wait 5
			Call CaptureImageUFTV2(Device("Device").App("BNI Agen46") , "User Pilih Maskapai Citilink", " ", compatibilityMode.Mobile, ReportStatus.Done)
			wait 5
		Else
			Reporter.ReportEvent micFail, "User Gagal Pilih Maskapai","Gagal"
		End If
	ElseIf dt_TCID = "MAG0026-002" or dt_TCID = "MAG0026-005" or dt_TCID = "MAG0026-007" or dt_TCID = "MAG0026-008" Then
		Do
		Loop until Device("Device").App("BNI Agen46").MobileObject("Garuda").Exist(1)
		If Device("Device").App("BNI Agen46").MobileObject("Garuda").Exist(5) Then
			Device("Device").App("BNI Agen46").MobileObject("Garuda").Tap
			wait 5
			Call CaptureImageUFTV2(Device("Device").App("BNI Agen46") , "User Pilih Maskapai Garuda", " ", compatibilityMode.Mobile, ReportStatus.Done)
			wait 5
		Else
			Reporter.ReportEvent micFail, "User Gagal Pilih Maskapai","Gagal"
		End If
	ElseIf dt_TCID = "MAG0026-003" or dt_TCID = "MAG0026-006" or dt_TCID = "MAG0026-009" Then
		Do
		Loop until Device("Device").App("BNI Agen46").MobileObject("Lion Air").Exist(1)
		If Device("Device").App("BNI Agen46").MobileObject("Lion Air").Exist(5) Then
			Device("Device").App("BNI Agen46").MobileObject("Lion Air").Tap
			wait 5
			Call CaptureImageUFTV2(Device("Device").App("BNI Agen46") , "User Pilih Maskapai Lion Air", " ", compatibilityMode.Mobile, ReportStatus.Done)
			wait 5
		Else
			Reporter.ReportEvent micFail, "User Gagal Pilih Maskapai","Gagal"
		End If
	Else
		Do
		Loop until Device("Device").App("BNI Agen46").MobileObject("Sriwijaya").Exist(1)
		If Device("Device").App("BNI Agen46").MobileObject("Sriwijaya").Exist(5) Then
			Device("Device").App("BNI Agen46").MobileObject("Sriwijaya").Tap
			wait 5
			Call CaptureImageUFTV2(Device("Device").App("BNI Agen46") , "User Pilih Maskapai Sriwijaya", " ", compatibilityMode.Mobile, ReportStatus.Done)
			wait 5
		Else
			Reporter.ReportEvent micFail, "User Gagal Pilih Maskapai","Gagal"
		End If
	End If
End Function

Function Input_PayCode(dt_TCID, dt_PayCode)
	Do
	Loop until Device("Device").App("BNI Agen46").MobileEdit("Kode_Bayar").Exist(1) 
	If Device("Device").App("BNI Agen46").MobileEdit("Kode_Bayar").Exist(5) Then
		Device("Device").App("BNI Agen46").MobileEdit("Kode_Bayar").Tap
		Device("Device").App("BNI Agen46").MobileEdit("Kode_Bayar").Set dt_PayCode
		
		If dt_TCID = "MAG0024-006" Then
			wait 5
			Call CaptureImageUFTV2(Device("Device").App("BNI Agen46") , "User Memasukan Kode Pembayaran Tidak Terdaftar", " ", compatibilityMode.Mobile, ReportStatus.Done)
			wait 5
		Else
			wait 5
			Call CaptureImageUFTV2(Device("Device").App("BNI Agen46") , "User Memasukan Kode Pembayaran", " ", compatibilityMode.Mobile, ReportStatus.Done)
			wait 5
		End If
		Device("Device").App("BNI Agen46").MobileButton("Lanjut").Tap
	Else
		Reporter.ReportEvent micFail, "User Gagal Memasukan Kode Pembayaran","Gagal"
	End If
End Function

Function Input_Pass(dt_TCID, dt_PinTrx)
	Do
	Loop until Device("Device").App("BNI Agen46").MobileEdit("PasswordTRX").Exist(1)
	If Device("Device").App("BNI Agen46").MobileEdit("PasswordTRX").Exist(5) Then
		Device("Device").App("BNI Agen46").MobileEdit("PasswordTRX").Tap
		wait 2
		Device("Device").App("BNI Agen46").MobileEdit("PasswordTRX").Set dt_PinTrx
		If dt_TCID = "MAG0023-004" or dt_TCID = "MAG0023-007" Then
			wait 5
			Call CaptureImageUFTV2(Device("Device").App("BNI Agen46") , "User Memasukan Password TRX Yang Salah", " ", compatibilityMode.Mobile, ReportStatus.Done)
			wait 5
		Else
			wait 5
			Call CaptureImageUFTV2(Device("Device").App("BNI Agen46") , "User Memasukan Password TRX", " ", compatibilityMode.Mobile, ReportStatus.Done)
			wait 5
		End If
		Device("Device").App("BNI Agen46").MobileObject("Lanjut_PassTRX").Tap
	End If
End Function

Sub Verify_Success()
	Do
	Loop until Device("Device").App("BNI Agen46").MobileObject("Beranda").Exist(1)
	If Device("Device").App("BNI Agen46").MobileObject("Beranda").Exist(5) Then
		wait 5
		Call CaptureImageUFTV2(Device("Device").App("BNI Agen46") , "Transaksi Berhasil", " ", compatibilityMode.Mobile, ReportStatus.Passed)
		wait 5
		Device("Device").App("BNI Agen46").MobileObject("Beranda").Tap
	Else
		Reporter.ReportEvent micFail, "Transaksi Gagal","Gagal"
	End If
End Sub

Sub Verify_Fail()
	AIUtil.SetContext Device("micclass:=Device")
	Do
	Loop until AIUtil("button", "Ok").Exist(1)
	If AIUtil("button", "Ok").Exist(5) Then
		wait 3
		Call CaptureImageUFTV2(Device("Device").App("BNI Agen46") , "Muncul Alert Error", " ", compatibilityMode.Mobile, ReportStatus.Passed)
		wait 3
		AIUtil("button", "Ok").Click
	Else
		Reporter.ReportEvent micFail, "Gagal Verifikasi Error","Gagal"
	End If
End Sub

