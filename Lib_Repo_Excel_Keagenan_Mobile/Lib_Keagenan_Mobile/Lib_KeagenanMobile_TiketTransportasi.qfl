		Call CaptureImageUFTV2(Device("Device").App("BNI Agen46") , "User Memasukan Nomer Rekening", " ", compatibilityMode.Mobile, ReportStatus.Done)
	End If
	wait 2
	Device("Device").App("BNI Agen46").MobileButton("Lanjut").Tap
