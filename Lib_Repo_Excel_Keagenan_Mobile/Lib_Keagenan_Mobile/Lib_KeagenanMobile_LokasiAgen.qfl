Sub Goto_LokasiAgen()
	Do
	Loop until Device("Device").App("BNI Agen46").MobileObject("Lokasi Agen").Exist(1)
	If Device("Device").App("BNI Agen46").MobileObject("Lokasi Agen").Exist(5) Then
		wait 5
		Call CaptureImageUFTV2(Device("Device").App("BNI Agen46") , "User Pilih Lokasi Agen", " ", compatibilityMode.Mobile, ReportStatus.Done)
		wait 5
		Device("Device").App("BNI Agen46").MobileObject("Lokasi Agen").Tap
	End If
End Sub

Sub Verivy_LokasiAgen()
	Do
	Loop until Device("Device").App("BNI Agen46").MobileObject("Lokasi Agen_Appear").Exist(1)
	If Device("Device").App("BNI Agen46").MobileObject("Lokasi Agen_Appear").Exist(5) Then
		wait 5
		Call CaptureImageUFTV2(Device("Device").App("BNI Agen46") , "User Berhasil Pilih Lokasi Agen", " ", compatibilityMode.Mobile, ReportStatus.Passed)
		wait 5
	End If
End Sub
